{
  "manifest_version": 2,
  "name": "Womp's Cemetech Userstyle",
  "version": "1.0.0",
  "description": "Adds a very spiffy userstyle to cemetech.net",
  "web_accessible_resources": ["DarkMode.css"],
  "permissions": ["storage"],
  "content_scripts": [{
    "css": ["userstyle.css"],
    "matches": ["https://www.cemetech.net/*"], 
    "js": ["wompScript.js"]
  }]
}